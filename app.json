{
  "expo": {
    "name": "elevenlabs-conversational-ai-expo-react-native",
    "slug": "elevenlabs-conversational-ai-expo-react-native",
    "version": "1.0.0",
    "orientation": "default",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app needs access to microphone for voice conversations."
      },
      "bitcode": false,
      "bundleIdentifier": "com.thorelevendev.elevenlabsconversationalaiexporeactnative"
    },
    "android": {
      "orientation": "default",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.CAMERA",
        "android.permission.INTERNET",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.WAKE_LOCK",
        "android.permission.BLUETOOTH"
      ],
      "package": "com.thorelevendev.elevenlabsconversationalaiexporeactnative"
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [],
    "extra": {
      "eas": {
        "projectId": "e398d043-2b54-4dea-a02b-2330b92cac38"
      }
    },
    "owner": "vishnu73394"
  }
}
